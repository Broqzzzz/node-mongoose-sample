<h2>Students List</h2>
{{#if students}}
  <!-- Looping through lists -->
  {{#each students}}
    <div class="row student">
      <div class="col-sm-2 center">
        <img src="{{ this.img }}" />
      </div>
      <div class="col-sm-10">
        <h4>{{this.name}}</h4>
        <p>{{this.id}}</p>
      </div>
    </div>
  {{/each}}
{{else}}
  <p>No students yet</p>
{{/if}}
